<h1>Edit Service Profile</h1>
<% if current_user.id != @walker.user_id %>
<h1 class="lead">Sorry, you cannot other users profiles.</h1>
<% else %>
<%= simple_form_for @walker, :html => { :multipart => true } do |f| %>
  <%= f.select :area_id, @areas %>
  <%= f.input :first_name, label: "First Name" %>
  <%= f.input :last_name, label: "Last Name" %>
  <%= f.input :phone %>
  <%= f.input :email %>
  <%= f.input :city %>
  <%= f.input :details %>
  <%= f.submit "Submit", class: "btn-primary" %>
<% end %>
<% end %>


